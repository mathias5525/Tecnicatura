Proceso Ejercicio_1
	Definir a,b,c,resultado Como Real;
	Escribir "Valor A: ";
	Leer a;
	Escribir "Valor B: ";
	Leer b;
	Escribir "Valor C: ";
	Leer c;
	resultado <- (- b + raiz(b^2-4*a*c))/(2*a);
	Escribir "EL RESULTADO DE LA OPERACION -A- ES: ", resultado;
	resultado <- (- b - raiz(b^2-4*a*c))/(2*a);
	Escribir "EL RESULTADO DE LA OPERACION -B- ES: ", resultado;
FinProceso
